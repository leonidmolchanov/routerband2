<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.routerband.routerband" version="2.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Mikrotik">
        <param name="android-package" value="com.javray.cordova.plugin.Mikrotik" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.sqlc.SQLitePlugin" />
    </feature>
    <feature name="PushbotsPlugin">
        <param name="android-package" value="com.pushbots.plugin.PushbotsPlugin" />
    </feature>
    <name>routerband</name>
    <description>
       Routerband
    </description>
    <author email="info@routerband.ru" href="http://routerband.ru">
        Routerband Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
